.content{
  padding: 0rpx 30rpx;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
}

.v1{
  background: rgb(241, 241, 241);
  color: rgb(95, 95, 95);
  border-radius: 1%;      
  -moz-border-radius: 1%; 
  -webkit-border-radius: 1%;
}

.v1-txt{
  padding: 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
}

.dot{
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;      
  -moz-border-radius: 50%; 
  -webkit-border-radius: 50%;
  background: rgb(95, 95, 95);
  margin-right: 10rpx;
  margin-top: 14rpx;
}

.weui-panel__hd::after {
  border-bottom: 0px;
}
