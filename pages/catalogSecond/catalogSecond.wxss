.content{
  padding: 0rpx 30rpx;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
}

.v1{
  background: rgb(241, 241, 241);
  color: rgb(95, 95, 95);
  border-radius: 1%;      
  -moz-border-radius: 1%; 
  -webkit-border-radius: 1%;
}

.v1-txt{
  padding: 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
}

.dot{
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;      
  -moz-border-radius: 50%; 
  -webkit-border-radius: 50%;
  background: rgb(95, 95, 95);
  margin-right: 10rpx;
  margin-top: 14rpx;
}

.line{
  display: block;
  width: 100%;
  height: 5rpx;
  background: rgb(229, 229, 229);
  margin: 26rpx 0px;
}

.bzImg{
  width: 60rpx;
  height: 80rpx;
  margin-left: 40rpx;
  margin-top: -20rpx;
}

.content-center{
  align-items: center;
}

.title-img{
  width: 130rpx;
  height: 150rpx;
}

.btn1{
  width: 350rpx;
  background: rgb(250, 109, 63);
  color: white;
}

.bntView{
  position:fixed;
  left:0;
  bottom:5rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
}